#include <iostream>
using namespace std;

// Recursive digit sum function
int recursiveDigitSum(string a) {
    if (a.size() == 1) {
        return a[0] - '0';
    } else {
        int sum = 0;
        for (int i = 0; i < a.size(); i++) {
            sum += a[i] - '0';
        }
        return recursiveDigitSum(to_string(sum));
    }
}

int main() {
    int c, k;
    cin >> c >> k;

    // Multiply first, then apply digit sum
    int product = c * k;
    cout << recursiveDigitSum(to_string(product)) << endl;
}
